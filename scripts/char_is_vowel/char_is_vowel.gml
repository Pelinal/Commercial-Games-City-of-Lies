function char_is_vowel(char){
	switch char {
		case "a":
			return true
			break;
		case "e":
			return true
			break;
		case "o":
			return true
			break;
		case "i":
			return true
			break;
		case "u":
			return true
			break;
		default:
			return false
			break;
	}
}